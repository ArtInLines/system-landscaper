{
	"name": "system-landscaper",
	"version": "0.1.1",
	"description": "System Landscaper Library",
	"main": "src/ui",
	"directories": {
		"doc": "docs"
	},
	"scripts": {
		"build": "npm run docs && npx tsc && webpack --config webpack.config.js",
		"dev1": "npx tsc --watch",
		"dev2": "webpack --config webpack.config.js --watch",
		"docs": "npx tsc --declaration --allowJs --emitDeclarationOnly --outDir types"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/artinlines/system-landscaper.git"
	},
	"keywords": [
		"graph",
		"system-landscape",
		"visualization",
		"graph-drawing"
	],
	"license": "BSD-3-Clause",
	"devDependencies": {
		"@babel/core": "^7.18.9",
		"@babel/preset-env": "^7.18.9",
		"@tsconfig/node18": "^1.0.1",
		"babel-loader": "^8.2.5",
		"ngraph.remove-overlaps": "^1.0.0",
		"typescript": "^4.9.4",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0"
	},
	"dependencies": {
		"gintersect": "0.1.0",
		"ngraph.centrality": "0.3.0",
		"ngraph.events": "0.0.3",
		"ngraph.forcelayout": "0.5.0",
		"ngraph.fromjson": "0.1.9",
		"ngraph.generators": "0.0.19",
		"ngraph.graph": "0.0.14",
		"ngraph.merge": "0.0.1",
		"ngraph.random": "0.0.1",
		"ngraph.tojson": "0.1.4",
		"simplesvg": "0.0.10"
	}
}
